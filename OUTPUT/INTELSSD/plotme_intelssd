set xlabel "Number of small files"
set ylabel "Time to remove small files (s)"
set title "Intel SSDSC2BW120A4 SSD file removal"
set key left
set output "deleting_small_files_intelssd.png"
set term png

plot [0:2500000][0:700] \
     'find_delete_intelssd.txt' using 1:5 smooth csplines \
      title "Intel SSD:find" , \
     'find_delete_intelssd.txt' using 1:5 notitle with points, \
     'rsync_empty_intelssd.txt' using 1:5 smooth csplines \
      title "Intel SSD:rsync" , \
     'rsync_empty_intelssd.txt' using 1:5 notitle with points, \
     'unlink_perl_intelssd.txt' using 1:5 smooth csplines \
      title "Intel SSD:Perl", \
     'unlink_perl_intelssd.txt' using 1:5 notitle with points
